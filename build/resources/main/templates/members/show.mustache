{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">email</th>
        <th scope="col">password</th>

    </tr>
    </thead>
    <tbody>
    {{#member}}
    <tr>
        <th scope="row">{{id}}</th>
        <td colspan="2">{{email}}</td>
        <td>{{password}}</td>
    </tr>
    {{/member}}
    </tbody>
</table>
<a href="/members">Go to Member List</a>
<a href="{{member.id}}/edit" class="btn btn-primary">Edit</a>
<a href="{{member.id}}/delete" class="btn btn-primary">Delete</a>
{{>layouts/footer}}